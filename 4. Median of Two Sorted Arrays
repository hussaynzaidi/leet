double findMedianSortedArrays(int* nums1, int nums1Size, int* nums2, int nums2Size) 
{
    int n=nums1Size+nums2Size;
    int m[n];
    int temp;
    for(int i=0;i<nums1Size;i++)
    {
        m[i]=nums1[i];
    }
    for(int i=0;i<nums2Size;i++)
    {
        m[i+nums1Size]=nums2[i];
    }
    for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(m[i]>m[j])
            {
                temp=m[i];
                m[i]=m[j];
                m[j]=temp;
            }
        }
    }
    double median;
    for(int i=0;i<n;i++)
    {
        if(n%2==0)
        {
           median=(m[n/2]+m[n/2-1])/2.0;
           
        }
        else{
            median=m[(n)/2];
            
        }
    }

    return median;
}
