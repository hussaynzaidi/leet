bool lemonadeChange(int* bills, int billsSize) {
    int f=0;
    int t=0;
    for(int i=0;i<billsSize;i++){
        if(bills[i]==5){
            f++;
        }
        if(bills[i]==10){
            t++;
            if(f){
                f--;
            }
            else{
                return false;
            }}
        if(bills[i]==20){
          
            if((f>=3) &&(t<1)){
                f-=3;
            }
            else if ((f>=1)&&(t>=1)){

                f--;
                t--;
            }
            else{
 
                return false;
            }
        }
        }
        return true;

    }
